generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_URL")
}

model Product {
  id        String   @id @default(uuid())
  title     String
  price     Int
  image     String
  description String
  category  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}